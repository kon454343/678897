function initTopBarPanel(){$("body").click(function(){var a=$(".contact_details");a.is(":visible")&&a.hide();a=$(".signin_box");a.is(":visible")&&a.hide()});$(".contact_details").click(function(a){a.stopPropagation()});$(".signin_box").click(function(a){a.stopPropagation()});$(".contact").click(function(a){$(".signin_box").hide();$(".contact_details").toggle();a.stopPropagation()});$(".contact_details .window-close").click(function(){$(".contact_details").hide()});$(".signin").click(function(a){$(".contact_details").hide();
$(".signin_box").toggle();a.stopPropagation()});$(".signin_box .window-close").click(function(){$(".signin_box").hide()})};