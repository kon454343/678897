            transform: translateZ(0);
            background-color: #fff;
            display: flex;
            flex-direction: column;
        }
        .app-shell.app-shell-hide {
            display: none;
        }
        .app-shell-header {
            position: relative;
            display: block;
            height: 56px;
            min-width: 320px;
            box-sizing: content-box;
            background-color: #ac1419;
        }
        .app-shell-header-wrapper {
