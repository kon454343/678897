            display: flex;
            align-self: center;
            list-style: none;
            margin: 0 0 15px 0;
            padding: 0;
        }
        @media (max-width: 767px) {
            app-shell-footer-navigation-group li {
                padding-right: 0;
            }
        }
        @media (min-width: 768px) {
            .app-shell-footer-navigation-group li {
                padding-right: 48px;
            }
        }
        @media (max-width: 767px) {
            .app-shell-phone-numbers-group li {
                width: 50%;
            }
        }
        @media (max-width: 979px) {
            .app-shell-footer-navigation-group {
                width: 100%;
            }
        }
        .app-shell-phone-number {
            color: #2f3234;
            font-weight: 900;
            line-height: 26px;
        }
        @media (max-width: 767px) {
            .app-shell-phone-number {
                font-size: 18px;
            }
        }
        @media (min-width: 768px) {
            .app-shell-phone-number {
                font-size: 22px;
            }
        }
        .app-shell-links {
            display: flex;
            list-style: none;
            margin: 0;
            padding: 0;
        }
        @media (max-width: 767px) {
            .app-shell-links {
                display: flex;
                flex-direction: column;
                width: 50%;
            }
            .app-shell-links .app-shell-link {
                padding-bottom: 10px;
            }
        }
        .app-shell-link {
            line-height: 21px;
        }
        @media (max-width: 767px) {
            .app-shell-link {
                font-size: 14px;
            }
        }
        @media (min-width: 768px) {
            .app-shell-link {
                font-size: 16px;
            }
        }
        .app-shell-bank-information {
            padding-bottom: 15px;
        }
        .app-shell-footer-info-group {
            font-size: 12px;
            line-height: 14px;
        }
        .app-shell-definition-group {
            display: flex;
            padding-top: 20px;
        }
        @media (max-width: 767px) {
            .app-shell-definition-group {
                flex-direction: column;
            }
        }
        .app-shell-codes {
            flex-grow: 1;
        }
