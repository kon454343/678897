                                <li>
                                    <div class="block buttonlike " tabindex="15" title="BNP Paribas" onclick="chooseFunction('13')"><i class="sprite-pbl bnx"></i></div>
                                </li>
                                <li>
                                    <div class="block buttonlike " tabindex="16" title="Płacę z Getin Bank" onclick="chooseFunction('14')"><i class="sprite-pbl gbx"></i></div>
                                </li>
                                <li>
                                    <div class="block buttonlike " tabindex="17" title="Płacę z Noble Bank" onclick="chooseFunction('15')"><i class="sprite-pbl nlx"></i></div>
                                </li>
